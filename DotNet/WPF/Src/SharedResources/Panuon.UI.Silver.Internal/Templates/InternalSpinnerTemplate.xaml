                                             Duration="0:0:1"
                                             RepeatBehavior="Forever" />
                            <DoubleAnimation Storyboard.TargetProperty="Percent"
                                             From="0.1"
                                             To="0.8"
                                             Duration="0:0:1.2"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Rotate"
                                             Storyboard.TargetProperty="Angle"
                                             Duration="0:0:0" />
                            <DoubleAnimation Storyboard.TargetProperty="Percent"
                                             Duration="0:0:0" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{x:Static irs:TemplateKeys.InternalSpinnerClassicTemplate}"
                     TargetType="{x:Type i:InternalSpinner}">
        <ControlTemplate.Resources>
            <Style x:Key="EllipseStyle"
                   TargetType="Ellipse">
                <Setter Property="Opacity"
                        Value="0" />
                <Setter Property="VerticalAlignment"
                        Value="Bottom" />
                <Setter Property="HorizontalAlignment"
                        Value="Center" />
                <Setter Property="Fill"
                        Value="{Binding Foreground, RelativeSource={RelativeSource AncestorType=i:InternalSpinner}, Mode=OneWay}" />
                <Setter Property="Width"
                        Value="{Binding Thickness, RelativeSource={RelativeSource AncestorType=i:InternalSpinner}, Mode=OneWay}" />
                <Setter Property="Height"
                        Value="{Binding Thickness, RelativeSource={RelativeSource AncestorType=i:InternalSpinner}, Mode=OneWay}" />
                <Setter Property="RenderTransformOrigin">
                    <Setter.Value>
                        <MultiBinding Converter="{StaticResource {x:Static irs:ConverterKeys.InternalSpinnerClassicRenderTransformOriginConverter}}">
                            <Binding Path="ActualHeight"
                                     RelativeSource="{RelativeSource AncestorType=i:InternalSpinner}" />
                            <Binding Path="Thickness"
                                     RelativeSource="{RelativeSource AncestorType=i:InternalSpinner}" />
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
            </Style>
        </ControlTemplate.Resources>
        <Grid>
            <Ellipse x:Name="ell1"
                     Style="{StaticResource EllipseStyle}">
                <Ellipse.RenderTransform>
                    <RotateTransform x:Name="rotate1"
                                     Angle="-90" />
                </Ellipse.RenderTransform>
            </Ellipse>
            <Ellipse x:Name="ell2"
                     Style="{StaticResource EllipseStyle}">
                <Ellipse.RenderTransform>
                    <RotateTransform x:Name="rotate2"
                                     Angle="-90" />
                </Ellipse.RenderTransform>
            </Ellipse>
            <Ellipse x:Name="ell3"
                     Style="{StaticResource EllipseStyle}">
                <Ellipse.RenderTransform>
                    <RotateTransform x:Name="rotate3"
                                     Angle="-90" />
                </Ellipse.RenderTransform>
            </Ellipse>
            <Ellipse x:Name="ell4"
                     Style="{StaticResource EllipseStyle}">
                <Ellipse.RenderTransform>
                    <RotateTransform x:Name="rotate4"
                                     Angle="-90" />
                </Ellipse.RenderTransform>
            </Ellipse>
            <Ellipse x:Name="ell5"
                     Style="{StaticResource EllipseStyle}">
                <Ellipse.RenderTransform>
                    <RotateTransform x:Name="rotate5"
                                     Angle="-90" />
                </Ellipse.RenderTransform>
            </Ellipse>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger  Property="IsSpinning"
                      Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard RepeatBehavior="Forever">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="rotate1"
                                                           Storyboard.TargetProperty="Angle">
                                <EasingDoubleKeyFrame KeyTime="0:0:0.4"
                                                      Value="70">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseIn" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:1.5"
                                                      Value="180" />
                                <EasingDoubleKeyFrame KeyTime="0:0:1.9"
                                                      Value="430">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:3.0"
                                                      Value="540" />
                                <EasingDoubleKeyFrame KeyTime="0:0:3.4"
                                                      Value="790">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:4.3"
                                                      Value="900" />
                                <EasingDoubleKeyFrame KeyTime="0:0:4.7"
                                                      Value="1150">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="ell1"
                                                           Storyboard.TargetProperty="Opacity">
                                <DiscreteDoubleKeyFrame KeyTime="0:0:0.2"
                                                        Value="1" />
                                <DiscreteDoubleKeyFrame KeyTime="0:0:4.45"
                                                        Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames BeginTime="0:0:0.18"
                                                           Storyboard.TargetName="rotate2"
                                                           Storyboard.TargetProperty="Angle">
                                <EasingDoubleKeyFrame KeyTime="0:0:0.4"
                                                      Value="70">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseIn" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:1.5"
                                                      Value="180" />
                                <EasingDoubleKeyFrame KeyTime="0:0:1.9"
                                                      Value="430">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:3.0"
                                                      Value="540" />
                                <EasingDoubleKeyFrame KeyTime="0:0:3.4"
                                                      Value="790">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <LinearDoubleKeyFrame KeyTime="0:0:4.3"
                                                      Value="900" />
                                <EasingDoubleKeyFrame KeyTime="0:0:4.7"
                                                      Value="1150">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseOut" />
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames BeginTime="0:0:0.18"
                                                           Storyboard.TargetName="ell2"
                                                           Storyboard.TargetProperty="Opacity">
                                <DiscreteDoubleKeyFrame KeyTime="0:0:0.2"
                                                        Value="1" />
                                <DiscreteDoubleKeyFrame KeyTime="0:0:4.45"
                                                        Value="0" />
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames BeginTime="0:0:0.36"
                                                           Storyboard.TargetName="rotate3"
